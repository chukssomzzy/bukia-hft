{
  "name": "bukia_hft",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only  ./src/index",
    "start": "ts-node --transpile-only src/index.ts",
    "worker": "ts-node-dev --respawn --transpile-only ./src/workers/index.ts",
    "test": "jest --forceExit",
    "typeorm": "ts-node  ./node_modules/typeorm/cli.js",
    "build": "tsc & node scripts/builds.js",
    "prod": "node build/index.js",
    "migrate": "ts-node  ./node_modules/typeorm/cli.js migration:run -d src/data-source",
    "migration:create": "ts-node  ./node_modules/typeorm/cli.js migration:create src/migrations/migration",
    "migration:generate": "ts-node  ./node_modules/typeorm/cli.js migration:generate src/migrations/migration -d src/data-source",
    "reset-src": "ts-node  ./node_modules/typeorm/cli.js schema:drop -d src/data-source && ts-node  ./node_modules/typeorm/cli.js schema:sync -d src/data-source",
    "migration:revert": "ts-node  ./node_modules/typeorm/cli.js migration:revert -d src/data-source",
    "prepare": "husky",
    "commitlint": "commitlint --edit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js --fix",
    "migrate:prod": "node ./node_modules/typeorm/cli.js migration:run -d build/data-source.js"
  },
  "author": "chukssomzzy <chukssomzzy@gmail.com>",
  "license": "ISC",
